                                                alt="">
                                            <img class="hover-img" style="height: 400px; width: 300px; "  src="/productAssets/<%= productList.images[1] %>"
                                                alt="">
                                        </a>
                                        <% if (productList.countInStock===0) { %>
                                            <!-- Show the "Out of Stock" message -->
                                            <span class="out-of-stock">Out of Stock</span>
                                            <% } %>
                                                <div class="action-links">
                                                    <% if (productList.countInStock !==0) { %>
                                                        <!-- Show the "Add to Cart" button -->
                                                        <input type="hidden" id="productId<%= i %>"
                                                            value="<%= productList._id %>">
                                                        <a class="cart-btn" title="Add to Cart"
                                                            onclick="addCart('<%= i %>')">
                                                            <i class="icon-basket-loaded"></i>
                                                        </a>
                                                        <% } %>
                                                            <% if (findWish[productList._id]) { %>
                                                                <a class="action-btn hover-up"
                                                                    title="Remove from Wishlist"
                                                                    id="removeFromWishlist_<%= productList._id %>"
                                                                    style="display: inline-block;"
                                                                    onclick="removeWishlist('<%= productList._id %>')">

                                                                    <i class="fa-solid fa-heart"
                                                                        style="color: #ff0000; font-size: 20px;"></i>
                                                                </a>
                                                                <a class="action-btn hover-up" title="Add to Wishlist"
                                                                    style="display: none;"
                                                                    onclick="addToWishlist('<%= productList._id %>','<%=findWish[productList._id]%>')">
                                                                    <i class="fa-solid fa-heart"
                                                                        style="font-size: 20px;"></i>
                                                                </a>
                                                                <% } else { %>
                                                                    <a class="action-btn hover-up"
                                                                        title="Remove from Wishlist"
                                                                        id="removeFromWishlist_<%= productList._id %>"
                                                                        style="display: none;"
                                                                        onclick="removeWishlist('<%= productList._id %>')">
                                                                        <i class="fa-solid fa-heart"
                                                                            style="color: #ff0000; font-size: 20px;"></i>
                                                                    </a>
                                                                    <a class="action-btn hover-up"
                                                                        title="Add to Wishlist"
                                                                        id="addToWishlist_<%= productList._id %>"
                                                                        style="display: inline-block;"
                                                                        onclick="addToWishlist('<%= productList._id %>','<%=findWish[productList._id]%>')">
                                                                        <i class="fa-solid fa-heart"
                                                                            style="font-size: 20px;"></i>
                                                                    </a>
                                                                    <% } %>
                                                                        <a href="#" class="quick-view"
