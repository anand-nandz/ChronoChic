        function updateOrderStatusTracking(status, expectedDeliveryDate) {
    // Update status text
    document.querySelector('.bg-secondary .text-center:nth-child(2)').innerHTML = '<span class="text-medium">Status:</span> ' + status;

    // Update expected delivery date
    document.querySelector('.bg-secondary .text-center:nth-child(3)').innerHTML = '<span class="text-medium">Expected Date:</span> ' + expectedDeliveryDate;

    // Update steps based on status
    const steps = document.querySelector('.steps');
    steps.innerHTML = ''; // Clear existing steps

    const statusSteps = {
        'Processing': 'Processing',
        'Order Confirmed': 'Confirmed Order',
        'Product Dispatched': 'Product Dispatched',
        'Shipped': 'Product shipped', // Assuming 'Shipped' leads to 'Product Delivered'
        'Delivered': 'Product Delivered',
        'Canceled': 'Order Canceled'
    };

    const currentStep = statusSteps[status];
    const stepTitles = Object.values(statusSteps);

    stepTitles.forEach(stepTitle => {
        const step = document.createElement('div');
        step.className = 'step' + (stepTitle === currentStep || stepTitles.indexOf(stepTitle) <= stepTitles.indexOf(currentStep) ? ' completed' : '');
        step.innerHTML = `
            <div class="step-icon-wrap">
                <div class="step-icon"><i class="pe-7s-cart"></i></div>
            </div>
            <h4 class="step-title">${stepTitle}</h4>
        `;
        steps.appendChild(step);
    });
}

