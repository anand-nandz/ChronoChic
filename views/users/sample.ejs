<script>

        
    function updateOrderStatusTracking(status, expectedDeliveryDate) {
    // Update status text
    document.querySelector('.bg-secondary .text-center:nth-child(2)').innerHTML = '<span class="text-medium">Status:</span> ' + status;

    // Update expected delivery date
    document.querySelector('.bg-secondary .text-center:nth-child(3)').innerHTML = '<span class="text-medium">Expected Date:</span> ' + expectedDeliveryDate;

    // Update steps based on status
    const steps = document.querySelector('.steps');
    steps.innerHTML = ''; // Clear existing steps

    const statusIcons = {
        'Processing': 'pe-7s-refresh-2',
        'Order Confirmed': 'pe-7s-cart',
        'Product Dispatched': 'pe-7s-plane',
        'Shipped': 'pe-7s-credit',
        'Delivered': 'pe-7s-check',
        'Canceled': 'pe-7s-close',
        'Returned': 'pe-7s-refresh'
    };

    const statusSteps = {
        'Processing': 'Processing',
        'Order Confirmed': 'Confirmed Order',
        'Product Dispatched': 'Product Dispatched',
        'Shipped': 'Product Shipped',
        'Delivered': 'Product Delivered',
        'Canceled': 'Order Canceled',
        'Returned': 'Order Returned'
    };

    // Determine if order is canceled, returned, or other status
    const isCanceled = status === 'Canceled';
    const isReturned = status === 'Returned';
    const isOtherStatus = !isCanceled && !isReturned;

    // Filter steps based on status
    const filteredSteps = Object.entries(statusSteps).filter(([stepStatus]) =>
        isCanceled ? stepStatus === 'Canceled' :
        isReturned ? stepStatus === 'Returned' :
        stepStatus !== 'Canceled' && stepStatus !== 'Returned'
    );

    // Add steps to DOM
    filteredSteps.forEach(([stepStatus, stepTitle]) => {
        const step = document.createElement('div');
        const iconClass = statusIcons[stepStatus] || 'pe-7s-angle-right'; // Default icon
        const completed = status === stepStatus || (isOtherStatus && statusSteps[status] === stepTitle);
        step.className = 'step' + (completed ? ' completed' : '');
        step.innerHTML = `
            <div class="step-icon-wrap">
                <div class="step-icon"><i class="${iconClass}"></i></div>
            </div>
            <h4 class="step-title">${stepTitle}</h4>
        `;
        steps.appendChild(step);
    });

    // Add canceled or returned step if necessary
    if (isCanceled || isReturned) {
        const step = document.createElement('div');
        const iconClass = statusIcons[status] || 'pe-7s-angle-right'; // Default icon
        const stepTitle = statusSteps[status];
        step.className = 'step completed';
        step.innerHTML = `
            <div class="step-icon-wrap">
                <div class="step-icon"><i class="${iconClass}"></i></div>
            </div>
            <h4 class="step-title">${stepTitle}</h4>
        `;
        steps.appendChild(step);
    }
}

// Example usage:
updateOrderStatusTracking(document.getElementById('orderstatus').value, 'SEP 09, 2017');
    
    
</script>












