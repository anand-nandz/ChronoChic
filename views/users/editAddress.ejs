    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const form = document.getElementById('editAddressForm');
            const submitButton = document.getElementById('submitButton');

            // Function to get element value by ID
            function getElementValue(id) {
                const element = document.getElementById(id);
                return element ? element.value.trim() : '';
            }

            // Function to validate input
            function validateInput(inputId, pattern, errorId, errorMessage) {
                const inputValue = getElementValue(inputId);
                const errorElement = document.getElementById(errorId);

                if (!inputValue) {
                    errorElement.innerText = errorMessage + ' is required.';
                    return false;
                } else if (pattern && !pattern.test(inputValue)) {
                    errorElement.innerText = 'Please enter a valid ' + errorMessage.toLowerCase() + '.';
                    return false;
                } else {
                    errorElement.innerText = '';
                    return true;
                }
            }

            // Function to enable/disable submit button based on validation
            function enableSubmitButton() {
                submitButton.disabled = !(
                    validateInput('name', /^[A-Za-z\s]+$/, 'nameError', 'Full Name') &&
                    validateInput('phone', /^(91)?0?[6-9]\d{9}$/, 'phoneError', 'Phone') &&
                    validateInput('altPhone', /^(91)?0?[6-9]\d{9}$/, 'altPhoneError', 'Alternate Phone') &&
                    validateInput('pincode', /^[1-9][0-9]{5}(?:\s[0-9]{3})?$/, 'pincodeError', 'Pincode (6 digits)') &&
                    validateInput('landMark', null, 'landMarkError', 'Landmark') &&
                    validateInput('city', /^[a-zA-Z\s]+$/, 'cityError', 'City') &&
                    validateInput('state', /^[a-zA-Z\s]+$/, 'stateError', 'State') &&
                    validateInput('addressType', null, 'addressTypeError', 'Address Type') &&
                    validateInput('homeAddress', null, 'homeAddressError', 'Address')
                );
            }

            // Event listeners for input fields
            form.querySelectorAll('input, select').forEach(input => {
                input.addEventListener('input', function () {
                    const errorId = this.id + 'Error';
                    const errorElement = document.getElementById(errorId);
                    if (errorElement) {
                        validateInput(this.id, null, errorId, ''); // Reset error message
                    }
                    enableSubmitButton();
                });
            });

            // Event listener for form submission
            form.addEventListener('submit', function (event) {
                if (!validateForm()) {
                    event.preventDefault(); // Prevent form submission if validation fails
                    // alert('Please fix validation errors before submitting the form.');
                    Swal.fire({
            icon: 'error',
            title: 'Validation Error',
            text: 'Please fix validation errors before submitting the form.',
            confirmButtonColor: '#3085d6',
            confirmButtonText: 'OK'
        });
                }
            });

            // Function to validate form
            function validateForm() {
                const isNameValid = validateInput('name', /^[A-Za-z\s]+$/, 'nameError', 'Full Name');
                const isPhoneValid = validateInput('phone', /^(91)?0?[6-9]\d{9}$/, 'phoneError', 'Phone');
                const isAltPhoneValid = validateInput('altPhone', /^(91)?0?[6-9]\d{9}$/, 'altPhoneError', 'Alternate Phone');
                const isPincodeValid = validateInput('pincode', /^[1-9][0-9]{5}(?:\s[0-9]{3})?$/, 'pincodeError', 'Pincode (6 digits)');
                const isLandMarkValid = validateInput('landMark', null, 'landMarkError', 'Landmark');
                const isCityValid = validateInput('city', /^[a-zA-Z\s]+$/, 'cityError', 'City');
                const isStateValid = validateInput('state', /^[a-zA-Z\s]+$/, 'stateError', 'State');
                const isAddressTypeValid = validateInput('addressType', null, 'addressTypeError', 'Address Type');
                const isHomeAddressValid = validateInput('homeAddress', null, 'homeAddressError', 'Address');

                // Check if phone and alternate phone are not the same
                const phoneValue = getElementValue('phone');
                const altPhoneValue = getElementValue('altPhone');
                if (phoneValue === altPhoneValue) {
                    document.getElementById('altPhoneError').innerText = 'Phone and Alternate Phone must be different.';
                    return false;
                }

                // Return true only if all inputs are valid
                return isNameValid && isPhoneValid && isAltPhoneValid && isPincodeValid && isLandMarkValid && isCityValid && isStateValid && isAddressTypeValid && isHomeAddressValid;
            }

        });





        const inputElement = document.getElementById('input');

        let timeoutId;

        function debounce(func, delay) {
            return function () {
                clearTimeout(timeoutId);
                timeoutId = setTimeout(() => {
                    func();
                }, delay);
            };
        }

        function showSearchResult() {
            const searchResult = document.querySelector('.search-result');
            searchResult.style.display = 'block';
        }

        function hideSearchResult() {
            const searchResult = document.querySelector('.search-result');
            searchResult.style.display = 'none';
        }

        function showNoProductFound() {
            const noProductFound = document.querySelector('.no-product-found');
            noProductFound.style.display = 'block';
        }

        function hideNoProductFound() {
            const noProductFound = document.querySelector('.no-product-found');
            noProductFound.style.display = 'none';
        }

        function SearchProduct() {
            const searchDataValue = inputElement.value.trim();


            if (searchDataValue !== "") {
                showSearchResult();
                debounce(() => {
                    console.log(searchDataValue)
                    $.ajax({
                        url: "/search",
                        method: "post",
                        data: {
                            searchDataValue
                        },
                        success: (response) => {
                            const searchResultsContainer = document.getElementById('searchResultsContainer')
                            searchResultsContainer.innerHTML = ""

                            if (response.searchProducts.length > 0) {
                                response.searchProducts.forEach(element => {
                                    const productContainer = document.createElement('div');
                                    productContainer.classList.add('product-container');

                                    const productElement = document.createElement('div');
                                    productElement.innerHTML = `
                                <a href="/productDetails?id=${element._id}" class="product-link">
                                    <img src="/productAssets/${element.images[0]}" alt="product">
                                    <div class="product-info">
                                        <p>${element.pname}</p>
                                        <p>₹${element.offprice}</p>
                                    </div>
                                `;

                                    productContainer.appendChild(productElement);
                                    searchResultsContainer.appendChild(productContainer);
                                });
                                hideNoProductFound();
                            } else {
                                hideSearchResult();
                                showNoProductFound();
                            }
                        }
                    })
                }, 500)();
            } else {
                hideSearchResult();
                hideNoProductFound();
            }
        }
    </script>


