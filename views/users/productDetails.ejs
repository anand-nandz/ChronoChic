    <script>
        function addCart(id){
            // const id = document.getElementById('productId').value
            const pdtOffPrice = document.getElementById('offerPrice').innerHTML

            // alert(pdtOffPrice)

            $.ajax({
                url:"/addCartLoad",
                method:"post",
                data:{
                    id,
                    pdtOffPrice,
                },
                success:(response)=>{
                    if(response.status === true){
                        window.location.href = "http://localhost:8080/cart"
                    }
                    else if (response.status == "viewCart") {
                        window.location.href = "http://localhost:8080/cart"
                }
                }
            })

        }




        function addToWishlist(id) {
            // alert(id)


            $.ajax({
                url: '/addToWishlist',
                method: 'post',
                data: {
                    id
                },
                success: (response) => {
                    if (response.status == true) {
                        const Toast = Swal.mixin({
                            toast: true,
                            position: "bottom",
                            showConfirmButton: false,
                            timer: 500,
                            didOpen: (toast) => {
                                toast.onmouseenter = Swal.stopTimer;
                                toast.onmouseleave = Swal.resumeTimer;

                            }
                        });
                        Toast.fire({
                            icon: "success",
                            title: "Add to wishlist"
                        }).then((result) => {

                           
                            document.getElementById("addToWishList_" + id).style.display = 'none';
                            document.getElementById("removeFromWishList_" + id).style.display = "inline-block";


                        })
                    }
                    else if (response.status == 400) {
                        Swal.fire({

                            text: "product is alredy in wishlist",
                            icon: "warning",
                            showCancelButton: true,
                            confirmButtonColor: "#3085d6",
                            cancelButtonColor: "#d33",
                            confirmButtonText: "Ok"
                        })
                            .then((result) => {
                                if (result.isConfirmed) {
                                    window.location.href = "/wishlist"
                                }
                            })
                    }
                }
            })
        }





        function removeWishlist(id) {
            // alert(id)
            // alert(findWish)
            $.ajax({
                url: "/removeWish",
                method: "post",
                data: {
                    id
                },
                success: (response) => {
                    if (response.status == true) {

                        const Toast = Swal.mixin({
                            toast: true,
                            position: "bottom",
                            showConfirmButton: false,
                            timer: 500,

                            // timerProgressBar: true,
                            didOpen: (toast) => {
                                toast.onmouseenter = Swal.stopTimer;
                                toast.onmouseleave = Swal.resumeTimer;
                            }
                        });
                        Toast.fire({
                            icon: "success",
                            title: "Remove from wishlist"
                        }).then((result) => {
                            
                            document.getElementById("addToWishList_" + id).style.display = "inline-block";
                            document.getElementById("removeFromWishList_" + id).style.display = 'none';

                        })
                    }
                }
            })
        }



        const inputElement = document.getElementById('input');

        let timeoutId;

        function debounce(func, delay) {
            return function () {
                clearTimeout(timeoutId);
                timeoutId = setTimeout(() => {
                    func();
                }, delay);
            };
        }

        function showSearchResult() {
            const searchResult = document.querySelector('.search-result');
            searchResult.style.display = 'block';
        }

        function hideSearchResult() {
            const searchResult = document.querySelector('.search-result');
            searchResult.style.display = 'none';
        }

        function showNoProductFound() {
            const noProductFound = document.querySelector('.no-product-found');
            noProductFound.style.display = 'block';
        }

        function hideNoProductFound() {
            const noProductFound = document.querySelector('.no-product-found');
            noProductFound.style.display = 'none';
        }

        function SearchProduct() {
            const searchDataValue = inputElement.value.trim();


            if (searchDataValue !== "") {
                showSearchResult();
                debounce(() => {
                    console.log(searchDataValue)
                    $.ajax({
                        url: "/search",
                        method: "post",
                        data: {
                            searchDataValue
                        },
                        success: (response) => {
                            const searchResultsContainer = document.getElementById('searchResultsContainer')
                            searchResultsContainer.innerHTML = ""

                            if (response.searchProducts.length > 0) {
                                response.searchProducts.forEach(element => {
                                    const productContainer = document.createElement('div');
                                    productContainer.classList.add('product-container');

                                    const productElement = document.createElement('div');
                                    productElement.innerHTML = `
                                <a href="/productDetails?id=${element._id}" class="product-link">
                                    <img src="/productAssets/${element.images[0]}" alt="product">
                                    <div class="product-infos">
                                        <p>${element.pname}</p>
                                        <p>₹${element.offprice}</p>
                                    </div>
                                `;

                                    productContainer.appendChild(productElement);
                                    searchResultsContainer.appendChild(productContainer);
                                });
                                hideNoProductFound();
                            } else {
                                hideSearchResult();
                                showNoProductFound();
                            }
                        }
                    })
                }, 500)();
            } else {
                hideSearchResult();
                hideNoProductFound();
            }
        }

    </script>


