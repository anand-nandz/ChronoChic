                    <div class="col-12">
                        <div class="account-dashboard">
                            <div class="row">
                                <div class="col-md-12 col-lg-12">
                                    <!-- Content directly shown without tabs -->
        
                                    <% if (typeof messages !== 'undefined' && messages.length > 0) { %>
                                        <div class="flash-messages">
                                            <% messages.forEach(message => { %>
                                                <div class="flash-message <%= message.type %>">
                                                    <%= message.text %>
                                                </div>
                                            <% }) %>
                                        </div>
                                    <% } %>
        
                                    <div class="tab-content dashboard-content">
                                        <div class="tab-pane fade show active" id="orders">
                                            <h3>Orders</h3>
                                            <div class="table-responsive">
                                                <table class="table" >
                                                    <thead>
                                                        <tr >
                                                            <th>Serial Number</th>
                                                            <th>Images</th>
                                                            <th>Order Number</th>
                                                            <th>Date</th>
                                                            <th>Status</th>
                                                            <th>Payment Type</th>
                                                            <th>Total Amount</th>
                                                            <th>Actions</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody >
                                                        <% let orderSerialNumber = (page - 1) * perPage + 1; %>
                                                        <% orders.forEach((order, index) => { %>
                                                            <tr>
                                                                <td><%= orderSerialNumber + index %></td>
                                                                <td>
                                                                    <div class="product-images-grid">
                                                                        <% for (let i = 0; i < Math.min(pdtDataArray[index].length, 3); i++) { %>
                                                                            <div class="product-item">
                                                                                <% if (pdtDataArray[index].length === 1) { %>
                                                                                    <div class="serial-number">1</div>
                                                                                <% } else if (pdtDataArray[index].length === 2) { %>
                                                                                    <div class="serial-number"><%= i + 1 %></div>
                                                                                <% } %>
                                                                                <div class="product-image">
                                                                                    <img src="/productAssets/<%= pdtDataArray[index][i].images[0] %>" alt="#" style="height: 80px; width: 80px;">
                                                                                </div>
                                                                                <div class="product-name">
                                                                                    <%= pdtDataArray[index][i].pname %>
                                                                                </div>
                                                                            </div>
                                                                        <% } %>
                                                                    </div>
                                                                </td>
                                                                <td><%= order.orderNumber %></td>
                                                                <td><%= order.orderDate %></td>
                                                                <td><%= order.status %></td>
                                                                <td><%= order.orderType %></td>
                                                                <td>₹<%= order.totalAmount %></td>
                                                                <td><a href="/orderView?id=<%= order._id %>" class="view">View</a></td>
                                                            </tr>
                                                        <% }); %>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
        
                                        <!-- Pagination area -->
                                        <% if (totalPage && page) { %>
                                            <div class="paginatoin-area">
                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12">
                                                        <ul class="pagination-box">
                                                            <% if (page > 1) { %>
                                                                <li><a href="/orders?page=<%= page - 1 %>">Previous</a></li>
                                                            <% } %>
                                                            <% for (let i = 1; i <= totalPage; i++) { %>
                                                                <li <% if (i === page) { %> class="active" <% } %>><a href="/orders?page=<%= i %>"><%= i %></a></li>
                                                            <% } %>
                                                            <% if (page < totalPage) { %>
                                                                <li><a href="/orders?page=<%= page + 1 %>">Next</a></li>
                                                            <% } %>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        <% } %>
                                    </div>
                                </div>
                            </div>
                        </div>
