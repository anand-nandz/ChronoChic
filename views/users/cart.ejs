    <script>
        function increment(pdtId, quantity, index, price, subtotal) {

            let qty = parseInt(document.getElementById("quantity" + index).innerHTML);
            // alert(qty)
            const sbt = document.getElementById("subtotal" + index).innerHTML
            // alert(sbt,"subtotal")
            const ttl = document.getElementById("total").innerHTML
            // alert(ttl,"totaal")
            const prc = document.getElementById("price" + index).innerHTML
            // alert(prc, "price")



            $.ajax({
                url: "/cartadd",
                method: "post",
                data: {
                    price,
                    pdtId,
                    qty,
                    subtotal,

                },
                success: (response) => {
                    if (response.status == true) {

                        document.getElementById("subtotal" + index).innerHTML = parseInt(prc) + parseInt(sbt)

                        document.getElementById("quantity" + index).innerHTML = qty + 1;

                        document.getElementById("cartSubtotal").innerHTML = response.total
                        document.getElementById("total").innerHTML = response.total

                    } else if (response.status == "minimum") {
                        Swal.fire({
                            icon: "error",
                            // title: "Oops...",
                            text: "We're sorry! Only 5 unit(s) allowed in each order!",

                        });

                    } else if (response.status == "stock") {

                        Swal.fire({
                            icon: "error",
                            title: "Oops...",
                            text: "Out of Stock!",

                        })


                    }
                }
            })
        }




        function decrement(pdtId, quantity, index, price, subtotal, event) {


            let qty = parseInt(document.getElementById("quantity" + index).innerHTML);


            // alert(qty)
            const sbt = document.getElementById("subtotal" + index).innerHTML
            // alert(sbt)
            const ttl = document.getElementById("total").innerHTML
            // alert(ttl)
            const prc = document.getElementById("price" + index).innerHTML
            // alert(prc)

            $.ajax({
                url: "/decrement",
                method: "post",
                data: {
                    pdtId,
                    qty,
                    index,
                    price,
                    subtotal
                },
                success: (response) => {
                    if (response.status == true) {

                        document.getElementById("subtotal" + index).innerHTML = parseInt(sbt) - parseInt(prc)
                        // document.getElementById("quantity" + index).innerHTML = parseInt(qty) - 1

                        document.getElementById("quantity" + index).innerHTML = qty - 1;

                        document.getElementById("cartSubtotal").innerHTML = response.total
                        document.getElementById("total").innerHTML = response.total


                    } else if (response.status == "minimum") {


                        Swal.fire({
                            icon: "error",
                            title: "Oops...",
                            text: "Minimum Quantity!",

                        });

                    }
                }
            })


        }



        function remove(id, index) {

            // alert(id)
            const sbt = document.getElementById("subtotal" + index).innerHTML

            Swal.fire({
                title: "Are you sure?",
                text: "You want to remove this item?!",
                icon: "warning",
                showCancelButton: true,
                confirmButtonColor: "#3085d6",
                cancelButtonColor: "#d33",
                confirmButtonText: "Yes, remove it!"
            }).then((result) => {
                if (result.isConfirmed) {
                    Swal.fire({
                        title: "Deleted!",
                        text: "Your item has been deleted.",
                        icon: "success"
                    }).then((result) => {

                        $.ajax({
                            url: "/pro-del",
                            method: "post",
                            data: {
                                id,
                                sbt
                            },
                            success: (response) => {
                                if (response.status == true) {

                                    window.location.href = "http://localhost:8080/cart"

                                    document.getElementById("total").innerHTML = response.total

                                }
                            }
                        })

                    })
                }
            });




        }



