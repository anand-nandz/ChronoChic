        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                    <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                </div>
                <div>
                    <!-- <a href="#" class="btn btn-light rounded font-md">Export</a> -->
                    <!-- <a href="#" class="btn btn-light rounded  font-md">Import</a> -->
                    <a href="/admin/products/add-product" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr width="100%">
                                    <th >Product Image</th>
                                    <th >Product Name</th>
                                    <th >Original Price</th>
                                    <th >Offer Price</th>
                                    <th>Discount Percentage</th>
                                    <th >Stock Left</th>
                                    <th >Listed/Unlisted</th>
                                    <th  class="text-end">Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for (let i = 0; i < allProducts.length; i++) { %>
                                    <tr>
                                        <td>
                                            <a class="itemside" href="#">
                                               
                                                    <img src="/productAssets/<%= allProducts[i].images[0] %>" class="img-sm img-thumbnail" style="height: 70px; width: 70px;" alt="Item">
                                               
                                               
                                            </a>
                                        </td>
                                        <td> 
                                            <h6 ><%= allProducts[i].pname %></h6>
                                            
                                        </td>
                                        <td><span>₹ <%= allProducts[i].price %></span></td>
                                        <td><span>₹ <%= allProducts[i].offprice %></span></td>
                                        <td><span><%= allProducts[i].discountPercentage %> % OFF</span></td>
                                        <td>
                                            <% if( (allProducts[i].countInStock) == 0 ) {%>
                                                <span class="badge rounded-pill alert-danger">Out of Stock</span>
                                            <% } else { %>
                                                <span class="badge rounded-pill alert-success d-inline-block px-3 py-2"><%= allProducts[i].countInStock %></span>
                                            <% } %>
                                            
                                        </td>
                                        <td>
                                            <% if (allProducts[i].is_listed) { %>
                                                <span class="badge rounded-pill alert-success">Listed</span>
                                            <% } else { %>
                                                <span class="badge rounded-pill alert-danger">Unlisted</span>
                                            <% } %>
                                        </td>
                                        <td class="text-end">
                                            <a href="/admin/products/edit-product?id=<%=allProducts[i]._id %>" class="btn btn-sm font-sm rounded btn-brand">
                                                <i class="material-icons md-edit"></i> Edit
                                            </a>
                                            <% if (allProducts[i].is_listed !== true) { %>
                                                <a href="/admin/delete-product/<%= allProducts[i]._id %>" class="btn btn-sm font-sm btn-light rounded">
                                                    <i class="material-icons md-delete_forever"></i> Delete
                                                </a>
                                            <% } %>
                                        </td>
                                    </tr>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
                <!-- card-body end// -->
            </div> <!-- card end// -->
            <div class="pagination-area mt-15 mb-50">
                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-center">
                        <% if (page > 1) { %>
                            <li class="page-item"><a class="page-link" href="/admin/products?page=<%= page - 1 %>"><i class="fa-solid fa-backward"></i></a></li>
                        <% } %>
                        <% for (let i = 1; i <= totalPage; i++) { %>
                            <li class="page-item <%= page === i ? 'active' : '' %>"><a class="page-link" href="/admin/products?page=<%= i %>"><%= i %></a></li>
                        <% } %>
                        <% if (page < totalPage) { %>
                            <li class="page-item"><a class="page-link" href="/admin/products?page=<%= page + 1 %>"><i class="fa-solid fa-forward"></i></a></li>
                        <% } %>
                    </ul>
                </nav>
