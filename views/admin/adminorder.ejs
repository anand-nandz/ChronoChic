                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Payment Method</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>

                                <% if (orderData) { %>

                                    <% for( let i = 0; i < orderData.length; i++ ) { %>

                                        <tr>
                                            <td><%= orderData[i].orderNumber %></td>
                                            <td><b><%= orderData[i].shippingAddress[0].name %></b></td>
                                            
                                            <td><a href="/cdn-cgi/l/email-protection" class="__cf_email__" data-cfemail="97faf6e5e1fef9d7f2eff6fae7fbf2b9f4f8fa"><%=orderData[i].userEmail%></a></td>
                                            <td><% if(orderData[i].orderType === "Razorpay") { %>
                                                <span><i class="material-icons md-payment font-xxl text-muted mr-5"></i> <%= orderData[i].orderType %></span>
                                                <% } else { %>
                                                    <span><img src="/adminassets/imgs/icons/cash-on-delivery.png" style="height: 16px; width: 15px;  align-items: center  ;" alt="">   <%= orderData[i].orderType %></span>

                                                <% } %>
                                            </td>
                                            <td>₹ <%=orderData[i].totalAmount %></td>
                                            <td>
                                                
                                                <% if (orderData[i].status === 'Delivered' || orderData[i].status === 'Shipped') { %>
                                                    <span class="badge rounded-pill alert-success"><%= orderData[i].status %></span>
                                                <% } else if (orderData[i].status === 'Canceled' || orderData[i].status === 'Returned') { %>
                                                    <span class="badge rounded-pill alert-danger"><%= orderData[i].status %></span>
                                                <% } else { %>
                                                    <span class="badge rounded-pill alert-warning"><%= orderData[i].status %></span>
                                                <% } %>



                                            </td>
                                            <td><%=orderData[i].orderDate %></td>
                                            <td class="text-end">
                                                <a href="/admin/orderDetails?id=<%= orderData[i]._id %>" class="btn btn-md rounded font-sm">Detail</a>
                                           
                                            </td>
                                        </tr>
                                    
                                    <% } %>
                                    
                                 
                                <% } %>
                                
                            


                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
            <div class="pagination-area mt-15 mb-50">
