        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Coupon</h2>
                    <p>Add, edit or delete a Coupon</p>
                </div>
                <div>
                    <input type="text" placeholder="Search Coupon" class="form-control bg-white">
                </div>
            </div>
            <div class="d-flex justify-content-end pb-20">
                <a href="/admin/addCoupon" class="btn btn-sm btn-primary rounded font-sm">Create Coupon</a>
            </div>
            <div class="card">
                <div class="card-body">
             
                        <div class="col-md-15">
                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr style="width: 100%;">
                                          
                                            <th>Coupon  Code</th>
                                            <th>Name</th>
                                            <th>Start Date</th>
                                            <th>End Date</th>
                                            <th>Min-amount</th>
                                            <th>Max-amount</th>
                                            <th>Status</th>
                                            <th class="text-end" >Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                      <% if (couponData.length>0) { %>

                                        <% for( let i = 0; i < couponData.length; i++ ) { %>

                                            <tr>
                                            
                                                <td><%= couponData[i].couponCode  %></td>
                                                <td><b><%= couponData[i].name  %></b></td>
                                                <td><%= couponData[i].startDate %></td>
                                                <td><%=  couponData[i].endDate %></td>
                                                <td><%=  couponData[i].minimumAmount %></td>
                                                <td><%=  couponData[i].maximumAmount%></td>
                                                <% if (couponData[i].isActive==true) { %>

                                                    <td id="status<%=i%>"><span class="badge rounded-pill alert-success">Active</span></td>
                                                    <td class="text-end" style="text-align: right;">
                                                        <a href="/admin/coupon-edit?id=<%=couponData[i]._id  %>" id="edit<%=i%>" class="btn btn-sm font-sm rounded btn-brand m-2">
                                                            <i class="material-icons md-edit"></i>EDIT</a>
                                                        <button type="button" class="btn btn-primary " style="padding: 4px 8px; display: inline-block;" id="listButton<%=i%>" onclick="Block('<%= couponData[i]._id %>','<%=i%>')">Not Active</button>
                                                      </td>
                                                 
                                                <% }else{ %>
                                                    <td id="status<%=i%>"><span class="badge rounded-pill alert-warning">Not Active</span></td>
                                                    <td class="text-end">
                                                        <a href="/admin/coupon-edit?id=<%= couponData[i]._id %>" id="edit<%=i%>" class="btn btn-sm font-sm rounded btn-brand m-2">
                                                            <i class="material-icons md-edit"></i>EDIT</a>
                                                        <button type="button" class="btn btn-primary" style="padding: 4px 8px; display: inline-block;" id="listButton<%=i%>" onclick="Block('<%=couponData[i]._id %>','<%=i%>')">Active</button>
                                                    </td>
                                                    <% } %>
                                               
                     
                                            </tr>

                                        
                                        <% } %>
                                       
                                      <% }else{ %>

                                        <tr>
                                            <td>
                                                No Coupon
                                            </td>
                                        </tr>

                                        <% } %>
                                                
                                        
                                          
                                    </tbody>
                                </table>
                            </div>
                        </div> 
                
                </div> 
            </div>
